package com.example.demo;

public class oseba {

	private String ime;
	private String priimek;
	private String telefonskaStevlka;
	
	public oseba(String ime, String priimek, String telefonskaStevlka) {
		super();
		this.ime = ime;
		this.priimek = priimek;
		this.telefonskaStevlka = telefonskaStevlka;
	}
	public String getIme() {
		return ime;
	}
	public void setIme(String ime) {
		this.ime = ime;
	}
	public String getPriimek() {
		return priimek;
	}
	public void setPriimek(String priimek) {
		this.priimek = priimek;
	}
	public String getTelefonskaStevlka() {
		return telefonskaStevlka;
	}
	public void setTelefonskaStevlka(String telefonskaStevlka) {
		this.telefonskaStevlka = telefonskaStevlka;
	}
	
}
